[32m+[m[32m                // Respond to meeting invitation[m
[32m+[m[32m                async function respondToMeetingInvite(invitationId, response) {[m
[32m+[m[32m                    const action = response === 'Accepted' ? 'accept' : 'decline';[m
[32m+[m[32m                    if (!confirm(`Are you sure you want to ${action} this meeting invitation?`)) {[m
[32m+[m[32m                        return;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    try {[m
[32m+[m[32m                        const formData = new FormData();[m
[32m+[m[32m                        formData.append('invitationId', invitationId);[m
[32m+[m[32m                        formData.append('response', response);[m
[32m+[m[41m                        [m
[32m+[m[32m                        const tokenInput = document.querySelector('input[name="__RequestVerificationToken"]');[m
[32m+[m[32m                        if (tokenInput) {[m
[32m+[m[32m                            formData.append('__RequestVerificationToken', tokenInput.value);[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                        const result = await fetch('@Url.Action("RespondToMeetingInvite", "Student")', {[m
[32m+[m[32m                            method: 'POST',[m
[32m+[m[32m                            body: formData[m
[32m+[m[32m                        });[m
[32m+[m
[32m+[m[32m                        const data = await result.json();[m
[32m+[m
[32m+[m[32m                        if (data.success) {[m
[32m+[m[32m                            showNotification(data.message, 'success');[m
[32m+[m[32m                            loadMeetingInvitations(); // Reload invitations[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            showNotification(data.message, 'error');[m
[32m+[m[32m                        }[m
[32m+[m[32m                    } catch (error) {[m
[32m+[m[32m                        console.error('Error responding to invitation:', error);[m
[32m+[m[32m                        showNotification('Error responding to invitation. Please try again.', 'error');[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                // Mark meeting as attended[m
[32m+[m[32m                async function markAsAttended(invitationId) {[m
[32m+[m[32m                    if (!confirm('Mark this meeting as attended? This will add it to your project progress.')) {[m
[32m+[m[32m                        return;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    try {[m
[32m+[m[32m                        const formData = new FormData();[m
[32m+[m[32m                        formData.append('invitationId', invitationId);[m
[32m+[m[41m                        [m
[32m+[m[32m                        const tokenInput = document.querySelector('input[name="__RequestVerificationToken"]');[m
[32m+[m[32m                        if (tokenInput) {[m
[32m+[m[32m                            formData.append('__RequestVerificationToken', tokenInput.value);[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                        const result = await fetch('@Url.Action("MarkMeetingAttended", "Student")', {[m
[32m+[m[32m                            method: 'POST',[m
[32m+[m[32m                            body: formData[m
[32m+[m[32m                        });[m
[32m+[m
[32m+[m[32m                        const data = await result.json();[m
[32m+[m
[32m+[m[32m                        if (data.success) {[m
[32m+[m[32m                            showNotification(data.message, 'success');[m
[32m+[m[32m                            setTimeout(() => location.reload(), 1500);[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            showNotification(data.message, 'error');[m
[32m+[m[32m                        }[m
[32m+[m[32m                    } catch (error) {[m
[32m+[m[32m                        console.error('Error marking as attended:', error);[m
[32m+[m[32m                        showNotification('Error marking meeting as attended. Please try again.', 'error');[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                // Show notification[m
[32m+[m[32m                function showNotification(message, type) {[m
[32m+[m[32m                    const color = type === 'success' ? '#28a745' : '#dc3545';[m
[32m+[m[32m                    const icon = type === 'success' ? 'fa-check-circle' : 'fa-exclamation-circle';[m
[32m+[m[41m                    [m
[32m+[m[32m                    const notif = document.createElement('div');[m
[32m+[m[32m                    notif.style.cssText = `[m
[32m+[m[32m                        position: fixed;[m
[32m+[m[32m                        top: 80px;[m
[32m+[m[32m                        right: 20px;[m
[32m+[m[32m                        background: white;[m
[32m+[m[32m                        padding: 18px 25px;[m
[32m+[m[32m                        border-radius: 12px;[m
[32m+[m[32m                        box-shadow: 0 6px 20px rgba(0,0,0,0.2);[m
[32m+[m[32m                        border-left: 5px solid ${color};[m
[32m+[m[32m                        z-index: 10000;[m
[32m+[m[32m                        animation: slideIn 0.3s ease;[m
[32m+[m[32m                        max-width: 400px;[m
[32m+[m[32m                    `;[m
[32m+[m[32m                    notif.innerHTML = `[m
[32m+[m[32m                        <div style="display: flex; align-items: center; gap: 12px;">[m
[32m+[m[32m                            <i class="fas ${icon}" style="color: ${color}; font-size: 1.5em;"></i>[m
[32m+[m[32m                            <span style="color: #333; font-weight: 600;">${message}</span>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                    `;[m
[32m+[m[41m                    [m
[32m+[m[32m                    document.body.appendChild(notif);[m
[32m+[m[41m                    [m
[32m+[m[32m                    setTimeout(() => {[m
[32m+[m[32m                        notif.style.animation = 'slideOut 0.3s ease';[m
[32m+[m[32m                        setTimeout(() => notif.remove(), 300);[m
[32m+[m[32m                    }, 4000);[m
[32m+[m[32m                }[m

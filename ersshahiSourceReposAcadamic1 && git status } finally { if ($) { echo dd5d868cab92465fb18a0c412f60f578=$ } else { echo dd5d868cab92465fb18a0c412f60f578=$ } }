[32m+[m[32m                        const result = await response.json();[m
[32m+[m[41m                        [m
[32m+[m[32m                        if (result.success && result.invitations && result.invitations.length > 0) {[m
[32m+[m[32m                            let html = '';[m
[32m+[m[41m                            [m
[32m+[m[32m                            result.invitations.forEach(invite => {[m

[32m+[m[32m                                const isPending = myResponse === 'Pending';[m
[32m+[m[41m                                [m
[32m+[m[32m                                let statusClass = isPending ? 'pending' : (isAccepted ? 'accepted' : 'attended');[m
[32m+[m[32m                                let statusText = isPending ? 'Awaiting Response' : (isAccepted ? 'Accepted' : myResponse);[m
[32m+[m[41m                                [m
[32m+[m[32m                                html += `<div class="meeting-invitation-card ${statusClass}">`;[m

[32m+[m[32m                                html += `    <div class="meeting-inv-title">`;[m
[32m+[m[32m                                html += `      <i class="fas fa-calendar-check"></i>`;[m
[32m+[m[32m                                html += `      ${invite.Title}`;[m
[32m+[m[32m                                html += `    </div>`;[m
[32m+[m[32m                                html += `    <div class="meeting-inv-status ${statusClass}">`;[m
[32m+[m[32m                                html += `      ${statusText}`;[m

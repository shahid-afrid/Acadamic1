[32m+[m[32m                                html += `      </div>`;[m
[32m+[m[32m                                html += `    </div>`;[m
[32m+[m[41m                                [m
[32m+[m[32m                                html += `    <div class="meeting-inv-detail">`;[m
[32m+[m[32m                                html += `      <i class="fas fa-calendar-alt"></i>`;[m
[32m+[m[32m                                html += `      <div class="meeting-inv-detail-content">`;[m
